<p id="notice"><%= notice %></p>

<h1>Properties</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address1</th>
      <th>Address2</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Phone</th>
      <th>Fax</th>
      <th>Email</th>
      <th>Check in at</th>
      <th>Check out at</th>
      <th>Night audit auto</th>
      <th>Night audit time</th>
      <th>Night audit emails</th>
      <th>Print reg cards auto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.name %></td>
        <td><%= property.address1 %></td>
        <td><%= property.address2 %></td>
        <td><%= property.city %></td>
        <td><%= property.state %></td>
        <td><%= property.zip %></td>
        <td><%= property.phone %></td>
        <td><%= property.fax %></td>
        <td><%= property.email %></td>
        <td><%= property.check_in_at %></td>
        <td><%= property.check_out_at %></td>
        <td><%= property.night_audit_auto %></td>
        <td><%= property.night_audit_time %></td>
        <td><%= property.night_audit_emails %></td>
        <td><%= property.print_reg_cards_auto %></td>
        <td><%= link_to 'Show', property %></td>
        <td><%= link_to 'Edit', edit_property_path(property) %></td>
        <td><%= link_to 'Destroy', property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property', new_property_path %>
